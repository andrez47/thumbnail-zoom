<RDF xmlns="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
  xmlns:em="http://www.mozilla.org/2004/em-rdf#">

  <Description about="urn:mozilla:install-manifest">
    <em:id>{E10A6337-382E-4FE6-96DE-936ADC34DD04}</em:id>
    <em:name>Thumbnail Zoom</em:name>
    <em:description>Zooms thumbnail pictures from Facebook, Twitter, MySpace, Amazon, Picasa, Flickr, LinkedIn and Hi5</em:description>
    <em:version>1.2</em:version>
    <em:creator>Andres Hernandez Monge</em:creator>
    <em:iconURL>chrome://imagezoom/skin/images/icon64.png</em:iconURL>
    <em:optionsURL>chrome://imagezoom/content/options.xul</em:optionsURL>
    <em:type>2</em:type>

    <!-- Firefox -->
    <em:targetApplication>
      <Description>
        <em:id>{ec8030f7-c20a-464f-9b0e-13a3a9e97384}</em:id>
        <em:minVersion>3.0</em:minVersion>
        <em:maxVersion>4.0b7</em:maxVersion>
      </Description>
    </em:targetApplication>

  </Description>
</RDF>
